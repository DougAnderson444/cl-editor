{
  "name": "cl-editor",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "mkdir": "mkdirp ./dist",
    "build": "npm run mkdir && rollup -c && npm run copy-file",
    "build:min": "cross-env NODE_ENV=production npm run mkdir && rollup -c && npm run copy-file",
    "copy-file": "copyfiles ./index.html ./dist",
    "clearn": "rimraf ./dist",
    "watch": "rollup -c -w",
    "serve": "live-server dist --port=3000",
    "start": "npm-run-all --parallel watch serve"
  },
  "files": [
    "index.html"
  ],
  "keywords": [
    "html editor",
    "svelte",
    "typescript"
  ],
  "author": "nenadpnc",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.26.0",
    "copyfiles": "^1.0.0",
    "cross-env": "^5.1.1",
    "install": "^0.8.2",
    "live-server": "^1.1.0",
    "mkdirp": "^0.5.1",
    "npm": "^5.5.1",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.5.4",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.4",
    "rollup-plugin-gzip": "^1.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-scss": "^0.3.0",
    "rollup-plugin-svelte": "^3.1.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "typescript": "^2.3.4"
  }
}
