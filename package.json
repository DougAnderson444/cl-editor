{
  "name": "cl-editor",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "copy": "copyfiles ./index.html ./dist",
    "serve": "live-server dist --port=3000",
    "dev": "rollup -c rollup.config.dev.js",
    "dev:w": "npm run copy && concurrently \"npm run dev -- -w\" \"npm run serve\"",
    "prod": "rollup -c rollup.config.prod.js"
  },
  "files": [
    "index.html"
  ],
  "keywords": [
    "html editor",
    "svelte",
    "typescript"
  ],
  "author": "nenadpnc",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.26.0",
    "concurrently": "^3.5.0",
    "copyfiles": "^1.0.0",
    "install": "^0.8.2",
    "live-server": "^1.1.0",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.4",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-svelte": "^3.1.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "typescript": "^2.3.4"
  }
}
